  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Mu.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="FSharp.Quotations.Evaluator" Version="2.1.0" />
  </ItemGroup>
